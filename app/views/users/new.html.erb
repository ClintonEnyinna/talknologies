<%= form_for(@user, url: signup_path, method: :post) do |f| %>
    <%= f.label :fullname %>
    <%= f.text_field :fullname %>
    </br>
    <%= f.label :username %>
    <%= f.text_field :username, class: 'form-control' %>
    </br>
    <%= f.submit 'Sign up', class: 'btn btn-primary mb-3'  %>
<% end %>